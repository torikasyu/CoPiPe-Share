{
  "name": "blob-uploader3",
  "version": "0.1.0",
  "description": "Azure Blob Storageにファイルをアップロードするデスクトップアプリケーション",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run watch\" \"npm run electron-dev\"",
    "electron-dev": "wait-on dist/main.js && electron .",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^8.0.1",
    "electron": "^28.0.0",
    "typescript": "^5.0.4",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
